{
    "name": "TEXTKING",
    "apiVersion": "1.0",
    "baseUrl": "https://api.textking.com/v1",
    "operations": {
        "GetProjects": {
            "httpMethod": "GET",
            "uri": "projects",
            "responseClass": "TextKing\\Model\\ProjectList",
            "additionalParameters": {
                "per_page": "query",
                "page": "query"
            }
        },
        "GetProject": {
            "httpMethod": "GET",
            "uri": "project/{projectId}",
            "responseClass": "TextKing\\Model\\Project",
            "parameters": {
                "projectId": {
                    "location": "uri",
                    "required": true
                }
            }
        },
        "CreateProject": {
            "httpMethod": "POST",
            "uri": "projects",
            "responseClass": "TextKing\\Model\\Project",
            "parameters": {
                "body": {
                    "location": "jsonBody",
                    "required": true
                }
            }
        },
        "UpdateProject": {
            "httpMethod": "PUT",
            "uri": "project/{projectId}",
            "responseClass": "TextKing\\Model\\Project",
            "parameters": {
                "projectId": {
                    "location": "uri",
                    "required": true
                },
                "body": {
                    "location": "jsonBody",
                    "required": true
                }
            }
        },
        "DeleteProject": {
            "httpMethod": "DELETE",
            "uri": "project/{projectId}",
            "parameters": {
                "projectId": {
                    "location": "uri",
                    "required": true
                }
            }
        },

        "GetTopics": {
            "httpMethod": "GET",
            "uri": "topics",
            "responseClass": "TextKing\\Model\\TopicList",
            "additionalParameters": {
                "per_page": "query",
                "page": "query"
            }
        },
        "GetTopic": {
            "httpMethod": "GET",
            "uri": "topic/{topicId}",
            "responseClass": "TextKing\\Model\\Topic",
            "parameters": {
                "topicId": {
                    "location": "uri",
                    "required": true
                }
            }
        },

        "GetSourceLanguages": {
            "httpMethod": "GET",
            "uri": "languages/source",
            "responseClass": "TextKing\\Model\\LanguageList",
            "additionalParameters": {
                "per_page": "query",
                "page": "query"
            }
        },
        "GetTargetLanguages": {
            "httpMethod": "GET",
            "uri": "languages/target",
            "responseClass": "TextKing\\Model\\LanguageList",
            "additionalParameters": {
                "per_page": "query",
                "page": "query"
            }
        },
        "GetLanguage": {
            "httpMethod": "GET",
            "uri": "language/{code}",
            "responseClass": "TextKing\\Model\\Language",
            "parameters": {
                "code": {
                    "location": "uri",
                    "required": true
                }
            }
        }
    }
}