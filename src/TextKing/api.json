{
    "name": "TEXTKING",
    "apiVersion": "1.0",
    "baseUrl": "https://api.textking.com/v1",
    "operations": {
        "GetTopics": {
            "httpMethod": "GET",
            "uri": "topics",
            "responseClass": "TextKing\\Model\\TopicList",
            "additionalParameters": {
                "per_page": "query",
                "page": "query"
            }
        },
        "GetTopic": {
            "httpMethod": "GET",
            "uri": "topic/{id}",
            "responseClass": "TextKing\\Model\\Topic",
            "parameters": {
                "id": {
                    "location": "uri",
                    "required": true
                }
            }
        },
        "GetSourceLanguages": {
            "httpMethod": "GET",
            "uri": "languages/source",
            "responseClass": "TextKing\\Model\\LanguageList",
            "additionalParameters": {
                "per_page": "query",
                "page": "query"
            }
        },
        "GetTargetLanguages": {
            "httpMethod": "GET",
            "uri": "languages/target",
            "responseClass": "TextKing\\Model\\LanguageList",
            "additionalParameters": {
                "per_page": "query",
                "page": "query"
            }
        },
        "GetLanguage": {
            "httpMethod": "GET",
            "uri": "language/{code}",
            "responseClass": "TextKing\\Model\\Language",
            "parameters": {
                "code": {
                    "location": "uri",
                    "required": true
                }
            }
        }
    }
}